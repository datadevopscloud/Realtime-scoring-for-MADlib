{
	"deploy-packages": [
		"model",
		"featuresengine",
		"featurecache"
	],
	"model": {
		"modeldb-datasource.jdbc-url": "jdbc:postgresql://{DB_HOST}:{PORT}/{DATABASE}",
		"modeldb-datasource.userName": "{DB_LOGIN}",
		"modeldb-datasource.password": "{DB_PWD}",
		"madlibrest.modelname": "Credit_fraud_randomforest_model",
		"madlibrest.modeldescription": "Random Forest Classification Example credit transactions",
		"madlibrest.modelschema": "credit_trans",
		"madlibrest.modeltables": [
			"rf_model",
			"rf_model_group",
			"rf_model_summary"
		],
		"madlibrest.modelinputtable": "feature_engine_test",
		"madlibrest.modelquery": "SELECT madlib.forest_predict('credit_trans.rf_model', 'credit_trans.feature_engine_test', 'credit_trans.model_test_scored', 'prob')",
		"madlibrest.resultstable": "model_test_scored",
		"madlibrest.resultsquery": "SELECT * from  credit_trans.model_test_scored, credit_trans.feature_engine_test"
	},
	"featuresengine": {
		"spring.profiles.active": "redis",
		"redis": {
			"clustertype": "standlone",
			"hostname": "{REDIS_HOST}",
			"port": 6379
		},
		"app.featurename": "CreditTransactionFeatures",
		"app.featuredescription": "Credit Transaction Features",
		"app.featuresschema": "credit_trans",
		"app.payloadtable": "message",
		"app.featurequery": "SELECT t.* ,m.m_fraud_cases ,abs(t.transaction_amount - m.m_avg_transaction_amount) AS m_transaction_delta,abs(t.transaction_amount - a.a_avg_transaction_amount) AS a_transaction_delta, CASE WHEN t.merchant_state = 'RS' THEN 1 ELSE 0 END AS merchant_state_2 FROM credit_trans.message AS t JOIN credit_trans.mf_merchants AS m USING (location_id) JOIN credit_trans.mf_accounts AS a USING (account_id)",
		"app.cacheenabled": "true",
		"app.cacheentities": {
			"mf_accounts": "account_id",
			"mf_merchants": "location_id"
		}
	},
	"featurecache": {
		"spring.profiles.active": "redis",
		"redis": {
			"clustertype": "standlone",
			"hostname": "{REDIS_HOST}",
			"port": 6379
		},
		"modeldb-datasource": {
			"jdbc-url": "jdbc:postgresql://{DB_HOST}:{PORT}/{DATABASE}",
			"username": "{DB_LOGIN}",
			"password": "{DB_PWD}"
		},
		"app": {
			"featurename": "CreditTransactionFeaturesCache",
			"featuresourceschema": "credit_trans",
			"featurefunctions": [
				"mf_accounts",
				"mf_merchants"
			],
			"featuresourcetables": {
				"mf_accounts": "account_id",
				"mf_merchants": "location_id"
			}
		}
	}
}
